<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<calibration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="/home/monica/Dropbox/configurationFiles/calibration.xsd">

  <algorithm>
      <simulatedAnnealing>
      <noEpsilon>4</noEpsilon>
      <rt>0.3</rt>
      <T>20</T>
      <NS>15</NS>
      <NT>5</NT>
      <epsilon>1.E-3</epsilon>
      <maxNoEval>200000</maxNoEval>
    </simulatedAnnealing>
  </algorithm>

  <NMSmodel>
    <type>
      <openLoop />
    </type>

    <tendon>
      <stiff/>
    </tendon>

    <activation>
      <exponential/>
    </activation>
  </NMSmodel>

  <samplingFrequency>
  200
  </samplingFrequency>

  <calibrationSteps>
    <step>
      <dofs> Knee_FE_R </dofs> 
      <objectiveFunction>
        <minimizeTorqueError/>
      </objectiveFunction>
      <strengthCoefficients_ShapeFactor_C1_C2_TendonSLackLength_single />
    </step>
    <step>
      <dofs> Knee_FE_R </dofs>
      <objectiveFunction>
        <minimizeTorqueError/>
      </objectiveFunction>
      <parameterSet> 
        <parameter>
          <name>strenghtCoefficients </name>
     <!--     <muscleGroups>  
            <muscles> a b c </muscles> 
            <muscles> c d e  </muscles> 
            <muscles> f g h </muscles> 
          </muscleGroups>
      -->    <absolute>  
            <range>-3 2.3</range>
          </absolute>
<!--          <relativeToSubjectValue>
            <range>0.95 1.10</range>
          </relativeToSubjectValue>
          -->
        </parameter> 
        <parameter>
          <name>shapeFactor</name>
          <muscleGroups>  
            <muscles> a b c </muscles> 
            <muscles> c d e  </muscles> 
            <muscles> f g h </muscles> 
          </muscleGroups>
   <!--       <absolute>  
            <range>-3 2.3</range>
          </absolute>
-->
         <relativeToSubjectValue>
            <range>0.95 1.10</range>
          </relativeToSubjectValue>
          
        </parameter> 
      </parameterSet>
    </step>
  </calibrationSteps>

  <trialSet>
    walking1 running2 crossover3
  </trialSet>


</calibration>
